{
  "name": "testingbotctl",
  "version": "1.0.0",
  "description": "CLI tool to run Espresso, XCUITest, and Maestro tests on TestingBot's cloud infrastructure",
  "main": "dist/index.js",
  "bin": {
    "testingbot": "dist/index.js"
  },
  "scripts": {
    "lint": "prettier --check '**/*.{js,ts}' && eslint --ext ts src/",
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "format": "prettier --write '**/*.{js,ts}'",
    "test": "jest",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "keywords": [
    "testingbot",
    "mobile-testing",
    "espresso",
    "xcuitest",
    "maestro",
    "android",
    "ios",
    "test-automation",
    "cloud-testing",
    "cli",
    "real-devices",
    "emulators",
    "simulators"
  ],
  "author": "TestingBot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/testingbot/testingbotctl.git"
  },
  "homepage": "https://testingbot.com",
  "bugs": {
    "url": "https://github.com/testingbot/testingbotctl/issues"
  },
  "dependencies": {
    "@types/archiver": "^7.0.0",
    "@types/js-yaml": "^4.0.9",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "figlet": "^1.9.4",
    "file-type": "^21.1.1",
    "form-data": "^4.0.5",
    "glob": "^13.0.0",
    "js-yaml": "^4.1.1",
    "prettier": "^3.7.4",
    "tracer": "^1.3.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "release-it": "^17.11.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
